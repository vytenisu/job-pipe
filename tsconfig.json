{
  "compilerOptions": {
    "target": "ES6",
    "lib": ["es2015", "es2017", "dom"],
    "sourceMap": true,
    "skipLibCheck": true,
    "noImplicitAny": true,
    "noImplicitUseStrict": true,
    "esModuleInterop": true,
    "typeRoots": ["./node_modules/@types"]
  },
  "include": ["./**/*.ts"],
  "exclude": ["**/*.test.ts", "node_modules", "cache", "build"]
}
